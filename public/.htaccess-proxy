<ifModule mod_rewrite.c>
    RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</ifModule>

<ifModule mod_rewrite.c>
 RewriteEngine On
 RewriteCond %{REQUEST_URI} ^/api/events/page_lang=en
 RewriteRule ^(.*)$ https://api.active-test.tech/$1 [P]
</ifModule>